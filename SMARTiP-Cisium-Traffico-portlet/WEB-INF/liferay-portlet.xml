<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>traffico</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/traffico.css</header-portlet-css>
		<header-portlet-css>http://code.jquery.com/ui/1.8.18/themes/base/jquery-ui.css</header-portlet-css>
		<header-portlet-css>/css/jquery.multiselect.css</header-portlet-css>
		<header-portlet-javascript>http://code.jquery.com/jquery-1.7.1.min.js</header-portlet-javascript>
		<header-portlet-javascript>http://code.jquery.com/ui/1.8.18/jquery-ui.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.multiselect.js</header-portlet-javascript>
		<header-portlet-javascript>/js/infobubble.js</header-portlet-javascript>
		<header-portlet-javascript>/js/TrafficForMap.js</header-portlet-javascript>	
		<footer-portlet-javascript>/js/ParcheggiForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/EventiForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/CiclopedpubForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/CameraForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/AccessibilityForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/FermateBusForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/LineeBusForMap.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/TaxiForMap.js</footer-portlet-javascript>
		<css-class-wrapper>traffico-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>